            "type": "string"
          },
          "size": {
            "type": "string",
            "enum": ["XS", "S", "M", "L", "XL", "XXL"]
          },
          "availability": {
            "type": "string",
            "enum": ["AVAILABLE", "UNAVAILABLE", "LOST", "SOLD", "RESERVED"]
          },
          "description": {
            "type": "string"
          },
          "color": {
            "type": "string"
          },
          "prize": {
            "type": "number",
            "format": "double"
          },
          "acquisitionDate": {
            "type": "string",
            "format": "date-time"
          },
          "state": {
            "type": "string",
            "enum": ["NEW", "USED"]
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "loan": {
            "$ref": "#/components/schemas/Loan"
          }
        }
      },
      "Loan": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "user": {
            "$ref": "#/components/schemas/User"
          },
          "startingDate": {
            "type": "string",
            "format": "date"
          },
          "endingDate": {
            "type": "string",
            "format": "date"
          },
          "state": {
            "type": "string",
            "enum": ["LOANED", "RETURNED", "EXPIRED", "PENDING"]
          },
          "clothingItems": {
            "uniqueItems": true,
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ClothingItem"
            }
          }
        }
      },
      "User": {
        "required": ["email", "name", "password"],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "maxLength": 20,
            "minLength": 3,
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "integer",
            "format": "int32"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "pattern": "^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$",
            "type": "string"
          },
          "clases": {
            "uniqueItems": true,
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Clase"
            }
          },
          "loans": {
            "uniqueItems": true,
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Loan"
            }
          },
          "role": {
            "type": "string",
            "enum": ["STUDENT", "TEACHER", "ADMIN"]
          },
          "active": {
            "type": "boolean"
          }
        }
      },
      "RegisterRequestDto": {
        "required": ["email", "name"],
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "phoneNumber": {
            "type": "integer",
            "format": "int32"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "maxLength": 2147483647,
            "minLength": 8,
            "pattern": "^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$",
            "type": "string"
          }
        }
      },
      "LoginRequestDto": {
        "required": ["email", "password"],
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "LoginResponseDto": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          },
          "tokenType": {
            "type": "string"
          },
          "userId": {
            "type": "integer",
            "format": "int64"
          },
          "username": {
            "type": "string"
          },
          "role": {
            "type": "string"
          }
        }
      }
    }
